<script>
/**
 * 显示微信
 */
function showwenxin(){
$.layer({
	type: 1,   //0-4的选择,
	title:  ["官方微信" , true],
	closeBtn: [0 , true],
	area: [550,345],
	offset : [ '100px', '' ],
	page: {
		dom : '#imgShowId'
	} 
});
}
/**
 * 提交-计算
 */
function fmSubmit(){
	//if (Validator.Validate('fm',4)==false) return;
	var _money=$("#money").val();
	var _rate=$("#rate").val();
	var _period=$("#period").val();
	var _lixi=_money*_rate*_period/12/100;
	var _bx=parseFloat(_money)+parseFloat(_lixi);
	$('#calcStep2').show();
	$('#calcStep1').hide();
	$("#bjI").html(_money+'元');
	$("#lvI").html(_rate+'%');
	$("#qxI").html(_period+'个月');
	$("#lxI").html(_lixi+'元');
	$("#bxI").html(_bx+'元');
}
</script>